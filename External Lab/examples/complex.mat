

def square(x: Mat(a, a)) -> Mat(a, a) {
    return x * x; 
}

def double(x: Mat(a, b)) -> Mat(a, b) {
    return x + x;
}

let x = [[1, 2, 3], [4, 5, 6], [7, 8, 9]];
let y = [[1, 2], [3, 4], [5, 6]];
let z = [[1, 2], [3, 4]];

# result: [[1148 1672]
#         [2606 3796]
#         [4064 5920]]
print(double(x) * y * square(z));

